public class For_mul {
	public static void main(String[] args) {
		int i,value=0;
		for(i=200;i>=100;i--) {
			value=value+i;
//			System.out.print(value*i);
//			System.out.print(a+i+i);
		}
		System.out.print(value);
	}

}
